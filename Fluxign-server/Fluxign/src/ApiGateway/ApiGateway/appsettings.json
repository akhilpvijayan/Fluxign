{
  "ReverseProxy": {
    "Routes": {
      "user-service-route": {
        "ClusterId": "user-service-cluster",
        "Match": {
          "Path": "/users/{**catch-all}"
        }
      }
      // Add more routes for other microservices here
    },
    "Clusters": {
      "user-service-cluster": {
        "Destinations": {
          "user-service-destination": {
            "Address": "https://localhost:5085/"
          }
        }
      }
      // Add more clusters here for other microservices
    },
    "Logging": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft.AspNetCore": "Warning"
      }
    }
  },
  "AllowedHosts": "*"
}
